<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lipi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddErrorActivity"
            android:label="Add Error">
        </activity>
        <activity android:name=".ViewErrorActivity"
            android:label="View Errors">
        </activity>
        <activity android:name=".ViewActivity"
            android:label="Search Machine"></activity>

        <activity android:name=".View2Activity"
            android:label="Search Error"></activity>
        <activity android:name=".View3Activity"
            android:label="Errors Max Occurred"></activity>
        <activity android:name=".ChartActivity"
            android:label="Pie Chart"></activity>
    </application>

</manifest>